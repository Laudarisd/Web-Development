{
    "name": "gh",
    "description": "GitHub command line tools.",
    "version": "1.13.9",
    "homepage": "http://nodegh.io",
    "author": {
        "name": "Eduardo Lundgren",
        "email": "eduardolundgren@gmail.com",
        "web": "http://eduardo.io",
        "twitter": "eduardolundgren"
    },
    "contributors": [
        {
            "name": "Zeno Rocha",
            "email": "zno.rocha@gmail.com",
            "web": "http://zenorocha.com",
            "twitter": "zenorocha"
        },
        {
            "name": "Ryan Garant",
            "email": "ryantgarant@gmail.com",
            "web": "https://github.com/protoEvangelion"
        }
    ],
    "license": "BSD-3-Clause",
    "engines": {
        "node": ">=6"
    },
    "preferGlobal": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/node-gh/gh.git"
    },
    "bugs": {
        "url": "https://github.com/node-gh/gh/issues"
    },
    "keywords": [
        "git",
        "github",
        "external",
        "commands",
        "helpers"
    ],
    "bin": {
        "gh": "bin/gh.js"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "release": {
        "branch": "master",
        "debug": true,
        "prepare": [
            "@semantic-release/npm",
            {
                "path": "@semantic-release/git",
                "assets": [
                    "package.json",
                    "bin/gh.js",
                    "lib/**/*.js"
                ],
                "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ],
        "publish": [
            "@semantic-release/github",
            "@semantic-release/npm"
        ]
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "commit": "git-cz",
        "format": "prettier 'lib/**/*.js' && prettier 'bin/*.js'",
        "env:test": "cross-env NODE_ENV=testing GH_USER=protoEvangelion GH_TOKEN=0001",
        "test": "npm run env:test -- jest",
        "test:watch": "npm run env:test -- jest --watch",
        "test:ci": "npm run env:test -- jest",
        "test:coverage": "npm run env:test -- jest --coverage",
        "test:program": "npm run env:test -- gh "
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "async": "1.5.2",
        "colors": "1.3.3",
        "github": "~0.2.4",
        "handlebars": "4.0.12",
        "inquirer": "6.2.1",
        "lodash": "4.17.11",
        "moment": "2.23.0",
        "nopt": "3.0.6",
        "opn": "5.4.0",
        "request": "2.88.0",
        "truncate": "2.0.1",
        "update-notifier": "2.5.0",
        "userhome": "1.0.0",
        "which": "1.3.1",
        "wordwrap": "1.0.0"
    },
    "devDependencies": {
        "@semantic-release/git": "7.0.7",
        "@semantic-release/github": "5.2.8",
        "@semantic-release/npm": "5.1.3",
        "@types/jest": "23.3.11",
        "@types/node": "10.12.18",
        "commitizen": "3.0.5",
        "cross-env": "~5.2.0",
        "cz-conventional-changelog": "2.1.0",
        "husky": "1.3.1",
        "jest": "23.6.0",
        "nock": "10.0.5",
        "prettier": "1.15.3",
        "pretty-quick": "1.8.0",
        "semantic-release": "15.13.2",
        "ts-jest": "23.10.5",
        "tslint": "5.12.0",
        "tslint-config-airbnb": "5.11.1",
        "tslint-config-prettier": "1.17.0",
        "tslint-plugin-prettier": "2.0.1",
        "typescript": "3.2.2"
    }
}
